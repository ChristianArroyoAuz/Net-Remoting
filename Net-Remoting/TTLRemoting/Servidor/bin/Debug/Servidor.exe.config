<configuration>
  <system.runtime.remoting>
    <application>
      <service>
        <wellknown
          type="Componente.Componente, Componente"
          objectUri="Componente"
          mode="Singleton" />
      </service>
      <channels>
        <channel
          ref="tcp"
          port="33000"/>
      </channels>
      <lifetime
        description="S-tiempo en segundos, M-min., H-horas, D-dias, MSmilisegundos, - S es el valor por defecto"
        leaseTime="5S"
        renewOnCallTime="5S"
        leaseManagerPollTime="1S"/>
    </application>
  </system.runtime.remoting>
</configuration>